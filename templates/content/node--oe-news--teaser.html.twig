{#
/**
 * @file
 * Theme override to display a node of bundle news in the teaser view mode.
 */
#}
{{ pattern('list_item', {
  'variant': 'thumbnail_primary',
  'url': url,
  'title': title,
  'meta': [
    content.oe_publication_date|field_value
  ],
  'detail': content.oe_teaser|field_value,
  'image': {
    'src': content.oe_news_featured_media|field_value|render
  }
}) }}
