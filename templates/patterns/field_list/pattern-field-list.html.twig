{#
/**
 * @file
 * Field list default variant.
 */
#}

{% set _items = [] %}
{% for _item in items %}
  {% set _body = _item.body|render %}
  {% if _body %}
    {% set _items = _items|merge([{
      term: _item.label,
      definition: _body
    }]) %}
  {% endif %}
{% endfor %}

{% if _items %}
  {% block content %}
  {% include '@ecl-twig/description-list' with {
    'variant': variant,
    'items': _items,
  } %}
  {% endblock %}
{% endif %}
