{#
/**
 * @file
 * Field list default variant.
 */
#}
{% set _items = [] %}
{% for _item in items %}
  {% set body = _item.body|render %}
  {% if body is not empty %}
    {% set _items = _items|merge([{
      term: _item.label,
      definition: body
    }]) %}
  {% endif %}
{% endfor %}
{% if _items %}
  {% include '@ecl-twig/description-list' with {
    'variant': variant,
    'items': _items,
  } %}
{% endif %}
