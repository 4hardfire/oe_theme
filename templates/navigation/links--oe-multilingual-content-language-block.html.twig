{#
/**
 * @file
 * Theme override for a set of links used in the language switcher block.
 *
 * @see ./core/modules/stable/navigation/links.html.twig
 */
#}
{{ attach_library('oe_theme/lang_select_pages') }}
{% if links -%}
  {% include '@ecl/ec-component-lang-select-page' with {
    is_primary: is_primary|default(true),
    unavailable: unavailable|default('English'),
    current: current|default(''),
    options: options|default([]),
    dropdown_label: dropdown_label|default('Select your language'|t),
    'extra_classes': extra_classes|default(''),
    'extra_attributes': extra_attributes|default([])
  } %}
{%- endif %}
