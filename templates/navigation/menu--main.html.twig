{#
/**
 * @file
 * Theme override to display a menu.
 *
 * @see ./core/themes/stable/templates/navigation/menu.html.twig
 */
#}
{{ attach_library('oe_theme/navigation') }}
{% if links %}
  {{ pattern('navigation_menu', {
    label: 'Menu'|t,
    items: links,
    number: 4,
    alignment: 'horizontal',
  }) }}
{% endif %}
